import datatype

MessageModel:
    (
        (imports*=Import)
        (msgs*=Message)
    )#
;

Message: PubSubMessage;

PubSubMessage:
    'TopicMsg' name=ID
        ('type:' dataType=[datatype.DataType|FQN])?     // Optional reference to a DataType
        properties*=Property                        // Optional additional or overriding properties
    'end'
;