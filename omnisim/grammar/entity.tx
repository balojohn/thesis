import datatype

EntityModel:
    (
        imports*=Import
        entities*=Entity
    )#
;

Entity:
    'Entity' name=ID
    (
        ('type:' etype=EntityType)
        ('topic:' topic=FQN)
        ('pubFreq:' pubFreq=FLOAT)?
        ('attributes:' '-' attributes*=Property['-'])?
    )#
    'end'
;

EntityType:
    'sensor' | 'actuator' | 'actor'
;