{% for sensor in sensors %}
Entity {{ sensor['name'] }}
    type: {{ sensor['type'] }}
    topic: {{ sensor['topic'] }}
    pubFreq: {{ sensor['pubFreq'] }}
    attributes:
    {% for attr in sensor['attributes'] %}
        - {{ attr[0] }}: {{ attr[1] }} = {{ attr[2] }}
    {% endfor %}
end
{% endfor %}

{% for actor in actors %}
Entity {{ actor['name'] }}
    type: {{ actor['type'] }}
    topic: {{ actor['topic'] }}
    pubFreq: {{ actor['pubFreq'] }}
    attributes:
    {% for attr in actor['attributes'] %}
        - {{ attr[0] }}: {{ attr[1] }}
    {% endfor %}
end
{% endfor %}

{% for actuator in actuators %}
Entity {{ actuator['name'] }}
    type: {{ actuator['type'] }}
    topic: {{ actuator['topic'] }}
    pubFreq: {{ actuator['pubFreq'] }}
    attributes:
    {% for attr in actuator['attributes'] %}
        - {{ attr[0] }}: {{ attr[1] }} = {{ attr[2] }}
    {% endfor %}
end
{% endfor %}